<div class='booyah-box-header col-10 offset-1'>
    <h1>Enter Your Place Details</h1>
</div>

<div class="booyah-box col-10 offset-1">
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    <%= f.submit 'Add Place', class: 'btn btn-primary' %>
  <% end %>
</div>